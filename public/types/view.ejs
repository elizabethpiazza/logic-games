<div class="container-fluid text-center">
	<h1>All logic games by type:</h1>

	<table class="table table-hover table-condensed">
		<thead>
			<tr>
				<th>Game Type</th>
				<th>Games</th>
			</tr>
		</thead>
		<tbody>
			<tr ng-repeat="type in types | orderBy: 'level'">
				<td><a href="#/types/{{type._id}}">{{type.typename}}</a></td>
				<td>{{type.games.length}}</td>
			</tr>
		</tbody>
	</table>
</div>